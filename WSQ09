print ("dame un numero y te dare su factorial")
x=int(input())
factorial=1
if x >0:
  for n in range (1,x+1):
    factorial=factorial*n
  print("el facotiral de ", x , "is", factorial)
  factorial=1
  print("deseas repetir si o no")
  res=input()
  while res==0("si)
    if res==("si"):
      print("dame otro numero")
      x=int (input())
      for n in range (1,x+1):
        factorial =factorial*n
      print("el factorial de ", x, "Es", factorial)
      print ("deseas repetir")
      res=input()
    else:
      print("adios")
    print("buen dia")
